;// $Id: highcharts.js 7 2013-06-11 07:56:08Z east $

Dida.module.highcharts.tableToChart = function(self) {
  var options = {
    chart: {type: self.attr('type') || 'spline', zoomType: 'x'},
    title: {text: self.attr('title') || ''},
    xAxis: {categories: []},
    yAxis: {title: {text: self.attr('ytitle') || ''}},
    tooltip: {
      crosshairs: true,
      formatter: function() { return "<b>" + this.series.name + "</b><br/><b>"+this.key+"：</b>" + this.y;}
    },
    series: [],
    exporting: {enabled: false }
  };

  var names = [];

  $('thead th', self).each(function(i, key) {
    if (i != 0) {
      names[names.length] = $(this).text();
    }
  });

  var data = [];
  $('tbody tr', self).each(function() {
    $('td', this).each(function(i, key) {
      if (!data[i]) data[i] = [];
      if (i != 0) {
        var v = parseFloat($(this).text().replace(/[^0-9]/g, ''));
        data[i][data[i].length] = {'y': (isNaN(v) || v == 0 ? null : v), 'name': data[0][data[i].length]};
      } else {
        data[i][data[i].length] = $(this).text();
      }
    });
  });

  for (var i in data) {
    if (i != 0) {
      //data[i].unshift(names[i]);
      options.series[options.series.length] = {'data': data[i], 'name': names[(i-1)]};
    } else {
      var k = parseInt(data[i].length/9); // 数据过多，省略坐标
      if (k > 1) {
        for (var j in data[i]) {
          options.xAxis.categories[j] = j%k ? '' : data[i][j];
        }
      } else {
        options.xAxis.categories = data[i];
      }
    }
  }

  if (self.attr('chartoptions')) {
    var p = eval('(' + self.attr('chartoptions').replace(/\\/g, '') + ')');
    if (typeof p == 'object') {
      $.extend(true, options, p);
    }
  }

  $('#' + self.attr('chartId')).highcharts(options);
}

$(function() {
  $('.dida_highcharts_data_table').each(function() {
    Dida.module.highcharts.tableToChart($(this));
  });

  $('.dida_highcharts_menu_labels').click(function() {
    var enableDataLabels = $(this).data('dataLabels');
    if (typeof(enableDataLabels) == 'undefined') enableDataLabels = true;
    var chart = $('#' + $(this).attr('chartId')).highcharts();
    if (chart) {
      for (var i in chart.series) {
        chart.series[i].update({
          dataLabels: {
            enabled: enableDataLabels
          }
        });
      }
    }
    $(this).data('dataLabels', !enableDataLabels);
    return false;
  });

  $('.dida_highcharts_menu_display').click(function() {
    var shows = $(this).data('shows');
    if (typeof(shows) == 'undefined') shows = true;
    var chart = $('#' + $(this).attr('chartId')).highcharts();
    if (chart) {
      for (var i in chart.series) {
        if (shows) {
          chart.series[i].hide();
        } else {
          chart.series[i].show();
        }
      }
    }
    $(this).data('shows', !shows);
    return false;
  });
});
;// $ID$
$(function() {
  $('#movie_stats_count_form_type_date').change(function() {
    if (confirm('是否切换到该日期？') !== false) {
      Dida.loca(Dida.url('user/center/movie_stats_type', {date: $(this).val()}));
    }
  });

  $('a.movie_stats_count_ajax_auto_data').click(function() {
    var id = $(this).attr('data');
    $.ajax({
      url: Dida.url('user/center/movie_stats_type/data'), 
      type: 'POST',
      dataType: 'json',
      data: $(this).parents('tr').find('input').serialize() + '&date=' + $('#movie_stats_count_form_type_date').val() + '&ext_type=' + $(this).attr('alt'),
      success: function(data) {
        if (data.error) {
          alert(data.error);
        } else {
          $.each(data, function(key, value) {
            $('#movie_stats_count_form_type_stats_' + id + '_' + key).val(value);
          });
        }
      }
    });
    return false;
  });

  $('.movie_stats_count_js_auto_data').click(function() {
    var id = $(this).attr('data') , type = $(this).attr('alt'), values = {};

    if (type == 'total_added') {
      values.total_added = parseInt($('#movie_stats_count_movie_form_type_stats_' + id + '_total_added').val());
      if (id == 0) {
        values.total = values.total_added;
      } else {
        values.total = parseInt($('#movie_stats_count_movie_form_type_stats_' + (id-1) + '_total').val())+values.total_added;
      }
    } else {
      values.total = parseInt($('#movie_stats_count_movie_form_type_stats_' + id + '_total').val());
      if (id == 0) {
        values.total_added = values.total;
      } else {
        values.total_added = values.total-parseInt($('#movie_stats_count_movie_form_type_stats_' + (id-1) + '_total').val());
      }
    }

    if (values.total_added && $('#movie_stats_count_movie_form_type_stats_' + id + '__price').val()) {
      values.people_added = Math.ceil(values.total_added/parseInt($('#movie_stats_count_movie_form_type_stats_' + id + '__price').val()));
      if (values.people_added && $('#movie_stats_count_movie_form_type_stats_' + id + '__people').val()) {
        values.show_added = Math.ceil(values.people_added/parseInt($('#movie_stats_count_movie_form_type_stats_' + id + '__people').val()));
      }
    }

    $.each(values, function(key, value) {
      $('#movie_stats_count_movie_form_type_stats_' + id + '_' + key).val(value);
    })

    return false;
  });
 
  $('#movie_stats_auto_data_trigger').click(function() {
    $('.movie_stats_count_js_auto_data').each(function() {
      if ($(this).attr('alt') == 'total_added') {
        $(this).trigger('click');
      }
    })
    return false;
  });

  var enableDataLabels = true;
  $('#chart_data_labels').click(function() {
    for (var i in Dida.module.highcharts.chart["chart"].series) {
      Dida.module.highcharts.chart["chart"].series[i].update({
        dataLabels: {
          enabled: enableDataLabels
        }
      });
    }
    enableDataLabels = !enableDataLabels;
    return false;
  });
});

;// $Id: editor.js 2 2012-12-09 19:42:54Z east $

// 所有编辑器实例
Dida.module.editor.instances = {}
