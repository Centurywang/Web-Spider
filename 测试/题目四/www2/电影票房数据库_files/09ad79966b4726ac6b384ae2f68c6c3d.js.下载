	      type: 'POST',
	      dataType: 'html',
	      url: Dida.url('ajax'),
	      data: 'module=system&op=block_delete_link&id=' + $$.attr('alt'),
	      success: function(data) {
	    		if (data) {
	    			$$.parent().remove();
	    		}
	      }
	    });
  	} else {
  		$$.parent().remove();
  	}
  });
  
