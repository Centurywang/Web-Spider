jQuery(function($) {
  if ($.fn.button.noConflict) {
    $.fn.btn = $.fn.button.noConflict();
  }

  $.fn.modal.Constructor.prototype.enforceFocus = function () {};
  
  $('.modal-link').live('click', function() {
    eval('var data = ' + ($(this).attr('source') || '{}') + ';');
    Dida.modal(data);
    return false;
  });

  $('.dida-modal-wrapper').live('hidden.bs.modal', function() {
    $(this).find('.btn-primary').off('click');
    $(this).find('.btn-cancel').off('click');
  });

  $('ul.site_menu > li > ul').each(function() {
    $(this).parent().addClass('dropdown');
    $(this).prev().addClass('dropdown-toggle').append('<b class="caret"></b>').attr('data-toggle', 'dropdown');
  });

  $('ul.site_menu_children > li > ul').each(function() {
    $(this).parent().addClass('dropdown-submenu');
  });

  $('ul.site_menu a.active').parent('li').addClass('active');
  $('ul.nav-tabs a.active, ul.nav-pills a.active').parent('li').addClass('active');
  $('ul.site_menu_children a.active').parents('.site_menu_list').addClass('active');
  $('.dropdown .item_dd_item_lang_change').addClass('dropdown-menu');

  $('#block_system_adminLink .item_admin_menus > h3').prepend('<i class="icon-plus"></i>');
});

Dida.modal = function(opt, callback) {
  var dom = '#'+(opt.dom || 'modal-wrapper');
  var button = '<button type="button" class="btn btn-default btn-cancel" data-dismiss="modal">'+Dida.t('system', '取消')+'</button>';
      button += '<button class="btn active btn-primary" type="button">'+Dida.t('system', '确认')+'</button>';

  $(dom).remove();
  var html = '';
  html = '<div class="modal fade dida-modal-wrapper" id="'+dom.substr(1)+'" aria-hidden="true" role="dialog">';
  html += '<div class="modal-dialog">';
  html += '<div class="modal-content">';
  html += '<div class="modal-header">';
  html += '<h3></h3>';
  html += '</div>';
  html += '<div class="modal-body"></div>';
  html += '<div class="modal-footer">';
  html += '<div class="button-wrapper">';
  html += button;
  html += '</div>';
  html += '</div>';
  html += '</div>';
  html += '</div>';
  html += '</div>';
  $('body').append(html);

  $(dom+' .button-wrapper').html(button);

  var data = {ok: Dida.t('system', '确认'), cancel: Dida.t('system', '取消'), options: {backdrop: 'static'}, title: 'Message'}

  $.extend(data, opt);

  if (data.content) {
    if (typeof data.content == 'string') {
      data.content = data.content.replace(/(\[\[|\]\])/g, function($0, $1) {
        if ($1 == '[[') {
          return '<';
        } else {
          return '>';
        }
      });
    }
    $(dom+' .modal-body').html(data.content);
  }

  $(dom).addClass('dida-modal-wrapper-hidden');

  if (opt.width) {
    $(dom+' .modal-dialog').css({width: opt.width+'px'});
  }
  
  if (opt.height) {
    $(dom+' .modal-body').css({height: opt.height+'px'});
  }

  var s = Dida.getsize();
  var h = $(dom+' .modal-dialog').height()+20;
  if (s[1] > h) {
    $(dom+' .modal-dialog').css('margin-top', ((s[1]-h)/2)+'px');
  } else {
    var h = s[1]-parseInt($(dom+' .modal-header').innerHeight())-parseInt($(dom+' .modal-footer').innerHeight())-50;
    $(dom+' .modal-body').css({height: h+'px', 'overflow-y': 'auto'});
    $(dom+' .modal-dialog').css('margin-top', '2px');
  }

  $(dom).removeClass('dida-modal-wrapper-hidden');

  if (data.title) {
    $(dom+' .modal-header h3').html(data.title);
  }

  if (data.callback && $.isFunction(data.callback)) {
    $(dom+' .btn-primary').on('click', function() {
      data.callback($(dom));
      return false;
    });
  }

  if (data.close && $.isFunction(data.close)) {
    $(dom+' .btn-cancel').on('click', function() {
      data.close($(dom));
    });
  }

  $(dom+' .btn-primary').text(data.ok);
  $(dom+' .btn-cancel').text(data.cancel);

  if (data.hidden_ok) {
    $(dom+' .btn-primary').hide();
  } else {
    $(dom+' .btn-primary').show();
  }

  if (data.hidden_cancel) {
    $(dom+' .btn-cancel').hide();
    $(dom+' li.next').removeClass('next').addClass('ok');
  } else {
    $(dom+' .btn-cancel').show();
    $(dom+' li.ok').addClass('next').removeClass('ok');
  }

  if (callback) callback($(dom));

  $(dom).modal(data.options || {});
}

;// $Id: scripts.js 2417 2015-03-21 10:12:47Z east $

jQuery(function($) {
  if (!Dida.settings.user_is_login) {
    var html = '你需要注册并登录才可以浏览完整的统计数据：<a href="';
        html += Dida.url('user/login', {redirect:location.pathname});
        html += '">立即登录</a>';
    $('.js_boxoffice_login_message').html(html).show();
  }
    
  //$('.js_boxoffice_ticket_link').html('<strong>我要购买电影票，最低仅10元<span class="label label-danger">new!</span></strong>').show();

  var _popoval = function(v) {
    if (!v) return '请输入';
    var text = '';
    for (var i = 0; i < v.length; i++) {
      text += ','+v.substr(i, 4);
      i += 3;
    }
    return text.substr(1);
  }

  $('input.movie_stats_count_numeric').popover({
    trigger: 'focus',
    html: true,
    placement: 'top',
    container: 'body',
    content: function() {
      return '<div id="movie_stats_count_filter_help">'+_popoval($(this).val())+'</div>';
    }
  });

  $('input.movie_stats_count_numeric').live('keyup focus', function() {
    var t = $(this).val(), text = '';

    if (!t) return false;

    if (parseInt(t) != t) {
      $(this).val($(this).attr('data-value') ? $(this).attr('data-value') : "");
      $('#movie_stats_count_filter_help').text('只能是数字');
      return;
    }

    $('#movie_stats_count_filter_help').text(_popoval(t));
  });

  $('#content_page_sub_tabs a.active').each(function() {
    var att = $(this).attr('href').split('/');
    $('#content_page_tabs a[href="/film/'+att[2]+'/'+att[3]+'"]').parent('li').addClass('active');
  });

  $('input:hidden').next('label.error').hide();
  if (window.location.pathname.indexOf('/boxoffice/') !== -1) {
    var type = [
          '/boxoffice/' + window.location.pathname.replace(/\/boxoffice\/(.*?)\/.*/ig, "$1"),
          '/boxoffice/' + window.location.pathname.replace(/\/boxoffice\/(.*?)\/(.*?)\/.*/ig, "$1/$2")
            ];
    $('.site_menu a, #block_movie_box_office a').each(function() {
      if ($.inArray($(this).attr('href'), type) !== -1) {
        $(this).addClass('js_menu_level_one_a_active');
        $(this).addClass('active');
      }
    });
  }

  $('#movie_goto_search_form').submit(function() {
    $.ajax({
      data: $(this).serialize() + '&search_goto=1',
      dataType: 'html',
      type: 'POST',
      url: '',
      success: function(data) {
        eval(data);
      }
    }); 
    return false;
  });

  $('#movie_date_change_form').submit(function() {
    var args = window.location.pathname.split('/');
    var date = $('#movie_date_change_form_type_date').val();
    var id = $('#movie_date_change_form_type_arg').val();
    args.shift();
    if (date != args[id]) {
      args[id] = date;
      window.location.href = Dida.url(args.join('/'));
    } else {
      $('#movie_date_change_form_type_date').focus();
    }
    return false;
  });

  if (Dida.settings.login_is_error) {
    Dida.messageShow(Dida.settings.login_is_error);
  }
 
});

;